{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ],
      "~/realmDB/*": [
        "./realmDB/*"
      ],
      "~/realmDB/model/*": [
        "./realmDB/model/*"
      ],
      "~/realmDB/repository/*": [
        "./realmDB/repository/*"
      ],
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
